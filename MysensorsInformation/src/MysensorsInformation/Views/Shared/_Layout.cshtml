@using Microsoft.ApplicationInsights.Extensibility
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>MysensorsInformation</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
    <link href="~/lib/angular-ui-grid/ui-grid.css" rel="stylesheet"/>
    <link href="~/lib/components-font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body ng-app="mysensorsInformationApp">
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <a asp-controller="Home" asp-action="Index" class="navbar-brand">Home</a>
            <a asp-controller="NRF24" asp-action="Index" class="navbar-brand">NRF24Sniffer</a>
            <a asp-controller="Mysensors" asp-action="Index" class="navbar-brand">MysensorListener</a>
        </div>
    </div>
</div>
<div class="body-content">
    @RenderBody()
</div>

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

<script src="~/lib/signalr/jquery.signalR.js"></script>
<script src="~/lib/angular/angular.js"></script>

<script src="~/lib/angular-ui-grid/ui-grid.js"></script>

<script src="~/js/app.js"></script>
<script src="~/js/services.js"></script>
<script src="~/js/homeClientController.js"></script>
<script src="~/js/mysensorClientController.js"></script>
<script src="~/js/nrf24ClientController.js"></script>

@RenderSection("scripts", required: false)
</body>
</html>
